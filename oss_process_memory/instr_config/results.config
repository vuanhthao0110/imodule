run_process = .bash_history : CONTAINS : ./script.sh
check_memory = .bash_history : CONTAINS : free -h
check_ram = .bash_history : CONTAINS : ps aux --sort=-%mem | head -n 10
clear_memory = .bash_history : CONTAINS : sudo sync; sudo sysctl -w vm.drop_caches=3
check_change = .bash_history : CONTAINS : free -h
